{
    "name": "mozhuilungdsuo/impersonator",
    "description": "A package to impersonate users",
    "keywords": [
        "mozhuilungdsuo",
        "impersonator"
    ],
    "homepage": "https://github.com/mozhuilungdsuo/impersonator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lungdsuo Mozhui",
            "email": "mozhui.lungdsuo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^8.0|^9.0|^10.0 | ^11.0",
    },

    "autoload": {
        "psr-4": {
            "Mozhuilungdsuo\\Impersonator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mozhuilungdsuo\\Impersonator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mozhuilungdsuo\\Impersonator\\ImpersonateServiceProvider"
            ]
           
        }
    }
}
